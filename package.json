{
  "name": "n8n-nodes-zalo-toolkit",
  "version": "0.1.2",
  "description": "Node for n8n Zalo integration",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "https://github.com/<...>/n8n-nodes-<...>#readme",
  "author": {
    "name": "hoaian",
    "email": "hoaian176@gmail.com",
    "url": "https://yourwebsite.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/<...>/n8n-nodes-<...>.git"
  },
  "engines": {
    "node": ">=20.15"
  },
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "npx rimraf dist && tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json",
    "watch": "concurrently \"pnpm run watch\" \"pnpm run start:n8n\"",
    "start:n8n": "nodemon --watch dist --exec \"npx n8n start\""
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/ExampleCredentialsApi.credentials.js",
      "dist/credentials/HttpBinApi.credentials.js",
      "dist/credentials/N8nApi.credentials.js",
      "dist/credentials/ZaloApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/ExampleNode/ExampleNode.node.js",
      "dist/nodes/HttpBin/HttpBin.node.js",
      "dist/nodes/ZaloLoginByQR/ZaloLoginByQR.node.js",
      "dist/nodes/ZaloSendMessage/ZaloSendMessage.node.js",
      "dist/nodes/ZaloUser/ZaloUser.node.js",
      "dist/nodes/ZaloGroup/ZaloGroup.node.js",
      "dist/nodes/ZaloMessageTrigger/ZaloMessageTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^24.2.1",
    "concurrently": "^9.1.2",
    "eslint": "^8.57.0",
    "gulp": "^5.0.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "n8n-core": ">=1.0.0",
    "n8n-workflow": "*"
  },
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/<...>/n8n-nodes-<...>/issues"
  }
}
